      <script>
         require([

           "esri/map",
           "esri/dijit/Search",
           "dojo/domReady!"

         ], function (Map, Search) {
            var map = new Map("map", {
               basemap: "gray",
               center: [-120.435, 46.159], // lon, lat
               zoom: 7
            });

            var search = new Search({
               map: map
            }, "search");
            search.startup();

         });
      </script>
      <style>
   html,
   body,
   #map {
      height: 100%;
      width: 100%;
      margin: 0;
      padding: 0;
   }
   #search {
      display: block;
      position: absolute;
      z-index: 2;
      top: 20px;
      left: 74px;
   }
</style>
